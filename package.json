{
  "name": "react-resume-template",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build": "yarn compile && yarn next build",
    "clean": "rm -rf build-tsc .next",
    "compile": "yarn run -T tsc --build --verbose",
    "dev": "yarn compile && yarn next dev",
    "lint": "yarn run -T prettier --write './src/**/*.{js,jsx,json,ts,tsx}' && eslint './src/**/*.{js,jsx,ts,tsx}' --fix --max-warnings=0",
    "start": "yarn next start",
    "sitemap": "yarn next-sitemap"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "classnames": "^2.5.1",
    "next": "^15.2.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ts-pattern": "^5.6.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.2.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.0.13",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.6",
    "eslint": "^9.22.0",
    "eslint-config-next": "^15.2.2",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-module-resolver": "^1.5.0",
    "eslint-plugin-monorepo": "^0.3.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-memo": "^0.0.3",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "postcss": "^8.5.3",
    "postcss-preset-env": "^10.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.85.1",
    "sort-package-json": "^3.0.0",
    "stylelint": "^16.15.0",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.3",
    "tailwindcss": "^4.0.13",
    "typescript": "^5.8.2"
  },
  "resolutions": {
    "autoprefixer": "10.4.5"
  }
}
